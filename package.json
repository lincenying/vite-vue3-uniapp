{
  "name": "vite-uniapp-vue3",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "serve": "uni",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-weixin": "uni build -p mp-weixin",
    "up": "pnpm up --latest --interactive \"!@dcloudio*\"",
    "lint": "eslint . --flag unstable_ts_config",
    "lint:fix": "eslint . --flag unstable_ts_config --fix",
    "lint:ts": "vue-tsc --noEmit --skipLibCheck",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-4030620241128001",
    "@dcloudio/uni-app-plus": "3.0.0-4030620241128001",
    "@dcloudio/uni-components": "3.0.0-4030620241128001",
    "@dcloudio/uni-h5": "3.0.0-4030620241128001",
    "@dcloudio/uni-mp-weixin": "3.0.0-4030620241128001",
    "@dcloudio/uni-ui": "^1.5.7",
    "@lincy/async-validation": "^1.3.0",
    "@lincy/tnui-vue3-uniapp": "^1.0.21",
    "@lincy/utils": "^0.6.1",
    "@tuniao/tn-icon": "^1.9.0",
    "@tuniao/tn-style": "^1.0.19",
    "@tuniao/tnui-vue3-uniapp": "^1.0.21",
    "@uni-helper/uni-use": "^0.19.14",
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.9",
    "fant-axios-adapter": "^0.0.6",
    "pinia": "2.3.0",
    "unhead": "^1.11.14",
    "vue": "3.5.13",
    "we-cropper": "^1.4.0",
    "wot-design-uni": "^1.5.1"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.14",
    "@dcloudio/uni-cli-shared": "3.0.0-4030620241128001",
    "@dcloudio/uni-stacktracey": "3.0.0-4030620241128001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4030620241128001",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/line-md": "^1.2.3",
    "@iconify-json/mdi": "^1.2.1",
    "@iconify-json/svg-spinners": "^1.2.1",
    "@lincy/eslint-config": "^5.3.1",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/uniapp-ui-resolver": "^1.2.1",
    "@lincy/unocss-base-config": "^2.0.0",
    "@types/node": "^22.10.2",
    "@unhead/vue": "^1.11.14",
    "@uni-helper/uni-app-types": "1.0.0-alpha.6",
    "@uni-helper/vite-plugin-uni-components": "^0.2.0",
    "@uni-helper/vite-plugin-uni-layouts": "^0.1.10",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.28",
    "@unocss/eslint-plugin": "^0.65.1",
    "@unocss/preset-mini": "0.64.0",
    "@vue-macros/volar": "^0.30.8",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "lint-staged": "^15.2.11",
    "miniprogram-api-typings": "^4.0.2",
    "pnpm": "^9.15.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "sass": "^1.83.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.6.3",
    "unocss": "^0.65.1",
    "unocss-applet": "^0.8.5",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "unplugin-vue-macros": "^2.13.6",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix --flag unstable_ts_config"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
