{
  "name": "vite-uniapp-vue3",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.2",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "serve": "uni",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-weixin": "uni build -p mp-weixin",
    "up": "pnpm up --latest --interactive \"!@dcloudio*\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit --skipLibCheck",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@dcloudio/uni-app": "catalog:prod",
    "@dcloudio/uni-app-plus": "catalog:prod",
    "@dcloudio/uni-components": "catalog:prod",
    "@dcloudio/uni-h5": "catalog:prod",
    "@dcloudio/uni-mp-weixin": "catalog:prod",
    "@dcloudio/uni-ui": "catalog:prod",
    "@iconify-json/carbon": "catalog:prod",
    "@iconify-json/line-md": "catalog:prod",
    "@iconify-json/mdi": "catalog:prod",
    "@iconify-json/svg-spinners": "catalog:prod",
    "@lincy/async-validation": "catalog:prod",
    "@lincy/tnui-vue3-uniapp": "catalog:prod",
    "@lincy/utils": "catalog:prod",
    "@tuniao/tn-icon": "catalog:prod",
    "@tuniao/tn-style": "catalog:prod",
    "@tuniao/tnui-vue3-uniapp": "catalog:prod",
    "@unhead/vue": "catalog:prod",
    "@uni-helper/uni-use": "catalog:prod",
    "@uni-helper/vite-plugin-uni-components": "catalog:prod",
    "@uni-helper/vite-plugin-uni-layouts": "catalog:prod",
    "@uni-helper/vite-plugin-uni-pages": "catalog:prod",
    "@vue/compiler-sfc": "catalog:prod",
    "@vueuse/core": "catalog:prod",
    "axios": "catalog:prod",
    "fant-axios-adapter": "catalog:prod",
    "pinia": "catalog:prod",
    "unhead": "catalog:prod",
    "vue": "catalog:prod",
    "we-cropper": "catalog:prod",
    "wot-design-uni": "catalog:prod"
  },
  "devDependencies": {
    "@dcloudio/types": "catalog:types",
    "@dcloudio/uni-cli-shared": "catalog:dev",
    "@dcloudio/uni-stacktracey": "catalog:dev",
    "@dcloudio/vite-plugin-uni": "catalog:dev",
    "@lincy/eslint-config": "catalog:dev",
    "@lincy/stylelint-config": "catalog:dev",
    "@lincy/uniapp-ui-resolver": "catalog:dev",
    "@lincy/unocss-base-config": "catalog:dev",
    "@types/node": "catalog:types",
    "@uni-helper/uni-app-types": "catalog:types",
    "@unocss/eslint-plugin": "catalog:dev",
    "@vue-macros/volar": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "lint-staged": "catalog:dev",
    "miniprogram-api-typings": "catalog:types",
    "pnpm": "catalog:dev",
    "postcss": "catalog:dev",
    "postcss-px-to-viewport-8-plugin": "catalog:dev",
    "sass": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:dev",
    "unocss-applet": "catalog:dev",
    "unplugin-auto-import": "catalog:dev",
    "unplugin-vue-components": "catalog:dev",
    "unplugin-vue-macros": "catalog:dev",
    "vite": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
