{
  "name": "vite-uniapp-vue3",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "author": "lincenying <lincenying@qq.com>",
  "scripts": {
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "serve": "uni",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-weixin": "uni build -p mp-weixin",
    "up": "pnpm up --latest --interactive \"!@dcloudio*\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc -b --noEmit --skipLibCheck",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "preinstall": "npx only-allow pnpm",
    "copy": "deploy_dist vite-uniapp-vue3/dist",
    "prepare[-del]": "npx simple-git-hooks"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-4060620250520001",
    "@dcloudio/uni-app-plus": "3.0.0-4060620250520001",
    "@dcloudio/uni-components": "3.0.0-4060620250520001",
    "@dcloudio/uni-h5": "3.0.0-4060620250520001",
    "@dcloudio/uni-mp-weixin": "3.0.0-4060620250520001",
    "@dcloudio/uni-ui": "^1.5.11",
    "@lincy/async-validation": "^1.4.0",
    "@lincy/utils": "^0.6.6",
    "@tuniao/tn-icon": "^1.11.0",
    "@tuniao/tn-style": "^1.0.20",
    "@tuniao/tnui-vue3-uniapp": "npm:@lincy/tnui-vue3-uniapp@1.0.23",
    "@uni-helper/uni-use": "^0.19.15",
    "@vue/compiler-sfc": "^3.5.24",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.13.2",
    "fant-axios-adapter": "^0.0.6",
    "pinia": "2.3.0",
    "tnuiv3p-tn-update-user-info-popup": "npm:@lincy/tnuiv3p-tn-update-user-info-popup@1.0.2",
    "unhead": "^2.0.19",
    "vue": "^3.5.24",
    "we-cropper": "^1.4.0",
    "wot-design-uni": "^1.13.0"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.28",
    "@dcloudio/uni-cli-shared": "3.0.0-4060620250520001",
    "@dcloudio/uni-stacktracey": "3.0.0-4060620250520001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4060620250520001",
    "@iconify-json/carbon": "^1.2.14",
    "@iconify-json/line-md": "^1.2.11",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@lincy/eslint-config": "^5.9.0",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/uniapp-ui-resolver": "^1.2.1",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/node": "^24.10.1",
    "@unhead/vue": "^2.0.19",
    "@uni-helper/uni-app-types": "1.0.0-alpha.6",
    "@uni-helper/vite-plugin-uni-components": "^0.2.3",
    "@uni-helper/vite-plugin-uni-layouts": "^0.1.11",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.29",
    "@unocss/eslint-plugin": "^66.5.6",
    "@vue-macros/volar": "^3.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.6",
    "miniprogram-api-typings": "^4.1.0",
    "pnpm": "^10.21.0",
    "postcss": "^8.5.6",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "sass": "^1.94.0",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.25.0",
    "typescript": "^5.9.3",
    "unocss": "66.0.0",
    "unocss-applet": "^0.12.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "6.3.5",
    "vue-tsc": "^3.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver",
      "vue-demi",
      "we-cropper"
    ]
  },
  "simple-git-hooks[-del]": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged[-del]": {
    "*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
