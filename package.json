    "@dcloudio/uni-stacktracey": "3.0.0-4020920240930001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4020920240930001",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/line-md": "^1.2.2",
    "@iconify-json/mdi": "^1.2.1",
    "@iconify-json/svg-spinners": "^1.2.1",
    "@lincy/base-config": "^1.3.2",
    "@lincy/eslint-config": "^5.2.6",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/uniapp-ui-resolver": "^1.1.1",
    "@types/node": "^22.8.2",
    "@unhead/vue": "^1.11.10",
    "@uni-helper/uni-app-types": "1.0.0-alpha.5",
    "@uni-helper/vite-plugin-uni-components": "^0.1.0",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.28",
    "@unocss/eslint-plugin": "^0.63.6",
    "@vue-macros/volar": "^0.30.5",
    "eslint": "^9.13.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "miniprogram-api-typings": "^4.0.1",
    "pnpm": "^9.12.3",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "sass": "^1.80.4",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unocss-applet": "^0.8.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "2.13.2",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
