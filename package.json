    "@dcloudio/uni-h5": "3.0.0-4020920240930001",
    "@dcloudio/uni-mp-weixin": "3.0.0-4020920240930001",
    "@dcloudio/uni-ui": "^1.5.6",
    "@lincy/async-validation": "^1.1.1",
    "@lincy/utils": "^0.6.1",
    "@lincy/vue-reuse-template": "^1.0.1",
    "@tuniao/tn-icon": "^1.9.0",
    "@tuniao/tn-style": "^1.0.19",
    "@tuniao/tnui-vue3-uniapp": "^1.0.21",
    "@uni-helper/uni-use": "^0.19.14",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "fant-axios-adapter": "^0.0.6",
    "pinia": "2.2.4",
    "unhead": "^1.11.10",
    "vue": "3.5.12",
    "we-cropper": "^1.4.0",
    "wot-design-uni": "^1.3.13"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.12",
